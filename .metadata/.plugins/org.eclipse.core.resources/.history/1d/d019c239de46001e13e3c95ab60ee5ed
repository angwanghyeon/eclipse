package ex01;

import java.io.IOException;
import java.net.Socket;

public class SimpleClient {
	
	public static void main(String[] args) {
		//try with resource는 트라이문에 에러문을 소괄호 안에 다 사용함
		try (Socket client = new Socket("localhost",5000);){
			
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
