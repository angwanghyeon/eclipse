/**
 * 
 */
function check_ok(){
	if(document.reg_frm.pers_1.value.length != 6){
		alert("주민번호 앞자리는 6글자입니다")
		reg_frm.pers_1.focus();
		return false;
	}
	if(document.reg_frm.pers_2.value.length != 7){
		alert("주민번호 뒷자리는 7글자입니다")
		reg_frm.pers_2.focus();
		return false;
	}
	if(document.reg_frm.id.value.length < 4){
		alert("아이디는 최소 4글자 이상 입력하세요")	
	}
}